{
  "name": "oss-client",
  "productName": "oss-client",
  "version": "0.0.1",
  "description": "OSS 云存储客户端",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "caorushizi",
    "email": "84996057@qq.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "oss_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "6.0.0-beta.47",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/classnames": "^2.2.9",
    "@types/mime": "^2.0.1",
    "@types/nedb": "^1.8.9",
    "@types/node": "^12.12.6",
    "@types/react-dom": "^16.9.4",
    "@types/react-motion": "0.0.29",
    "@types/react-redux": "^7.1.6",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "7.1.9",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0",
    "url-loader": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "electron-squirrel-startup": "^1.0.0",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "qiniu": "^7.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
